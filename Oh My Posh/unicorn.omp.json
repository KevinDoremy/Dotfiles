{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#003543",
                    "foreground": "#fff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "windows": "Óò™"
                    },
                    "style": "powerline",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
                    "type": "os"
                },
                {
                    "background": "#0087D8",
                    "foreground": "#003544",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "folder_separator_icon": "/",
                        "style": "full"
                    },
                    "style": "powerline",
                    "template": " Óóø {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#d2ff5e",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
                        "{{ if gt .Ahead 0 }}#f17c37{{ end }}",
                        "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
                    ],
                    "foreground": "#193549",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}Ôöí {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} ÔÜª {{ .WorktreeCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "background": "#003543",
                    "foreground": "#fff",
                    "powerline_symbol": "ÓÇ∞",
                    "style": "powerline",
                    "template": "<#fff> Ôôè </>{{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "background": "#83769c",
                    "foreground": "#ffffff",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": "<transparent>ÓÇ∞</> Ô®û{{ .FormattedMs }} ",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#fff",
                    "properties": {
                        "root_icon": "‚ö°"
                    },
                    "style": "plain",
                    "template": " ÔÉß ",
                    "type": "root"
                },
                {
                    "foreground": "#f1184c",
                    "style": "plain",
                    "template": "üîπ ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "console_title_template": "{{.User}}@{{.Host}} in {{ .PWD }}",
    "final_space": true,
    "version": 2
}